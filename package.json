{
    "name": "psel-xp-backend",
    "version": "0.1.0",
    "engines": {
        "node": ">=16.0.0"
    },
    "engineStrict": true,
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Fernando-Olmedo/psel-XP-BackEnd.git"
    },
    "homepage": "https://github.com/Fernando-Olmedo/psel-XP-BackEnd#readme",
    "main": "src/server.js",
    "scripts": {
        "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
        "start": "node .",
        "drop": "npx sequelize-cli db:drop",
        "seed": "npx sequelize-cli db:seed:all",
        "test": "nyc --all --include src/controllers --include src/services --include src/utilities --include src/middlewares mocha 'tests/**/*.js' --recursive --timeout 60000 --exit",
        "test-coverage": "jest -i --verbose --forceExit --coverage",
        "debug": "nodemon .",
        "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
    },
    "dependencies": {
        "argon2": "^0.28.7",
        "dotenv": "^16.0.0",
        "express": "4.17.1",
        "express-async-errors": "^3.1.1",
        "http-status-code": "^2.1.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.1.0",
        "sequelize": "^6.3.4"
    },
    "devDependencies": {
        "chai": "^4.3.6",
        "eslint": "6.8.0",
        "eslint-config-airbnb-base": "14.2.0",
        "eslint-config-trybe-backend": "1.0.3",
        "eslint-plugin-import": "2.22.1",
        "jest": "26.0.1",
        "jest-each": "^28.0.1",
        "mocha": "^10.0.0",
        "nodemon": "^2.0.15",
        "nyc": "^15.1.0",
        "sequelize-cli": "^6.2.0",
        "sinon": "^14.0.0"
    }
}
